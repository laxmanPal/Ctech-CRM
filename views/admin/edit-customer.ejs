<%- include("../includes/head.ejs") %>
<%- include("../includes/header.ejs") %>
<%- include("../includes/sidebar.ejs") %>
<div class="contents">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="breadcrumb-main">
          <h4 class="text-capitalize breadcrumb-title">Edit Customer</h4>
          <div class="breadcrumb-action justify-content-center flex-wrap">
            <nav aria-label="breadcrumb">
              <ol class="breadcrumb">
                <li class="breadcrumb-item">
                  <a href="#"><i class="uil uil-estate"></i>Home</a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">
                  form layouts
                </li>
              </ol>
            </nav>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <div class="card card-Vertical card-default card-md mb-4">
          <div class="card-body py-md-30">
            <form action="/admin/edit-customer" method="post">
              <input type="hidden" name="id" value="<%= customer.id %>">
              <div class="row">
                <div class="col-md-6 mb-25">
                  <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">Customer Name<span style="color: red;" >*</span></label>
                  <input
                    type="text"
                    name="cust_name"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="Customer Name"
                    value="<%= customer.name %>"
                  />
                </div>
                <div class="col-md-6 mb-25">
                  <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">Email Address<span style="color: red;" >*</span></label>
                  <input
                    type="email"
                    name="cust_email"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="Email"
                    value="<%= customer.email %>"
                  />
                </div>
                <div class="col-md-6 mb-25">
                  <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">Phone 1<span style="color: red;" >*</span></label>
                  <input
                    type="number"
                    name="cust_phone_1"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="Mobile"
                    value="<%= customer.phone_1 %>"
                  />
                </div>
                <div class="col-md-6 mb-25">
                  <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">Phone 2</label>
                  <input
                    type="number"
                    name="cust_phone_2"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="Mobile"
                    value="<%= customer.phone_2 %>"
                  />
                </div>
                <div class="col-md-6 mb-25">
                  <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">Phone 3</label>
                  <input
                    type="number"
                    name="cust_phone_3"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="Mobile"
                    value="<%= customer.phone_3 %>"
                  />
                </div>
                <div class="col-md-6 mb-25">
                  <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">Phone 4</label>
                  <input
                    type="number"
                    name="cust_phone_4"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="Mobile"
                    value="<%= customer.phone_4 %>"
                  />
                </div>
                  <div class="col-md-6 mb-25">
                    <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">Company Name<span style="color: red;" >*</span></label>
                    <input
                      type="text"
                      name="comp_name"
                      class="form-control ih-medium ip-gray radius-xs b-light px-15"
                      placeholder="Company Name"
                      value="<%= customer.comp_name %>"
                    />
                  </div>
                  <div class="col-md-6 mb-25">
                    <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">Address</label>
                    <input
                      type="text"
                      name="address"
                      class="form-control ih-medium ip-gray radius-xs b-light px-15"
                      placeholder="Address"
                      value="<%= customer.address %>"
                    />
                  </div>
                  <div class="col-md-6 mb-25">
                    <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">City</label>
                    <input
                      type="text"
                      name="city"
                      class="form-control ih-medium ip-gray radius-xs b-light px-15"
                      placeholder="City"
                      value="<%= customer.city %>"
                    />
                  </div>
                  <div class="col-md-6 mb-25">
                    <label for="formGroupExampleInput" class="color-dark fs-14 fw-500 align-center mb-10">Pincode<span style="color: red;" >*</span></label>
                    <input
                      type="number"
                      name="pincode"
                      class="form-control ih-medium ip-gray radius-xs b-light px-15"
                      placeholder="Pincode"
                      value="<%= customer.pincode %>"
                    />
                  </div>
                <div class="col-md-6">
                  <div class="layout-button mt-0">
                    <button
                      type="button"
                      class="btn btn-default btn-squared btn-light px-20"
                    >
                      cancel
                    </button>
                    <button
                      class="btn btn-primary btn-default btn-squared px-30"
                    >
                      save
                    </button>
                  </div>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%- include("../includes/footer.ejs") %>