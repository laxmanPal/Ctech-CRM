<%- include("../includes/head.ejs") %> <%- include("../includes/header.ejs") %>
<%- include("../includes/sidebar.ejs") %>
<div class="contents">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="breadcrumb-main">
          <h4 class="text-capitalize breadcrumb-title">Add New Receipt</h4>
          <div class="breadcrumb-action justify-content-center flex-wrap">
            <nav aria-label="breadcrumb">
              <ol class="breadcrumb">
                <li class="breadcrumb-item">
                  <a href="#"><i class="uil uil-estate"></i>Home</a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">
                  Add New Receipt
                </li>
              </ol>
            </nav>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <div class="card card-Vertical card-default card-md mb-4">
          <div class="card-body py-md-30">
            <form action="/customer/add-moneyReceipt" method="post">
              <!-- <input type="hidden" name="cust_id" value=""> -->
              <div class="row">
                <div class="col-md-6 mb-25">
                  <label
                    for="formGroupExampleInput"
                    class="color-dark fs-14 fw-500 align-center mb-10"
                    >Client Name</label
                  >
                  <input
                    type="text"
                    name="client_name"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="Client Name"
                  />
                </div>
                <div class="col-md-6 mb-25">
                  <label
                    for="formGroupExampleInput"
                    class="color-dark fs-14 fw-500 align-center mb-10"
                    >Mobile No.
                  </label>
                  <input
                    type="number"
                    name="client_mobile"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="Enter Mobile Number"
                  />
                </div>
                <div class="col-md-6 mb-25">
                  <label
                    for="formGroupExampleInput"
                    class="color-dark fs-14 fw-500 align-center mb-10"
                    >From</label
                  >
                  <input
                    type="text"
                    name="shift_from"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="From"
                  />
                </div>
                <div class="col-md-6 mb-25">
                  <label
                    for="formGroupExampleInput"
                    class="color-dark fs-14 fw-500 align-center mb-10"
                    >To</label
                  >
                  <input
                    type="text"
                    name="shift_to"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="To"
                  />
                </div>

                <div class="col-md-6 mb-25">
                  <label
                    for="formGroupExampleInput"
                    class="color-dark fs-14 fw-500 align-center mb-10"
                    >Price In Words </label
                  >
                  <input
                    type="number"
                    name="priceInWords"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    placeholder="Price In Words"
                  />
                </div>
                <div class="col-md-6 mb-25">
                  <label
                    for="formGroupExampleInput"
                    class="color-dark fs-14 fw-500 align-center mb-10"
                    >Shifting Date</label
                  >
                  <input
                    type="text"
                    name="shifting_date"
                    class="form-control ih-medium ip-gray radius-xs b-light px-15"
                    id="datepicker7"
                    placeholder="Shifting Date"
                  />
                </div>
                <div class="col-md-12 mb-25 d-flex">
                  <div class="col-md-4 mb-25">
                    <label
                      for="formGroupExampleInput"
                      class="color-dark fs-14 fw-500 align-center mb-10"
                      >Total </label
                    >
                    <input
                      type="number"
                      name="total"
                      class="form-control ih-medium ip-gray radius-xs b-light px-15"
                      placeholder="Total"
                    />
                  </div>
                  <div class="col-md-4 mb-25">
                    <label
                      for="formGroupExampleInput"
                      class="color-dark fs-14 fw-500 align-center mb-10"
                      >Advance </label
                    >
                    <input
                      type="number"
                      name="advance"
                      class="form-control ih-medium ip-gray radius-xs b-light px-15"
                      placeholder="Total"
                    />
                  </div>
                  <div class="col-md-4 mb-25">
                    <label
                      for="formGroupExampleInput"
                      class="color-dark fs-14 fw-500 align-center mb-10"
                      >Balance </label
                    >
                    <input
                      type="number"
                      name="balance"
                      class="form-control ih-medium ip-gray radius-xs b-light px-15"
                      placeholder="Total"
                    />
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="layout-button mt-0">
                    <button
                      type="button"
                      class="btn btn-default btn-squared btn-light px-20"
                    >
                      cancel
                    </button>
                    <button
                      class="btn btn-primary btn-default btn-squared px-30"
                    >
                      save
                    </button>
                  </div>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%- include("../includes/footer.ejs") %>
